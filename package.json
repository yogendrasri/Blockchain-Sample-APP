{
    "name": "fabcar",
    "version": "1.0.0",
    "description": "FabCar application implemented in JavaScript",
    "engines": {
        "node": "10.16.3",
        "npm": "6.9.0"
    },
    "scripts": {
        "start": "NODE_CONFIG_DIR=./server/config node server/server.js",
        "dev": "cross-env NODE_CONFIG_DIR=./server/config nodemon server/server.js",
        "lint": "eslint .",
        "pretest": "npm run lint",
        "test": "nyc mocha --recursive"
    },
    "engineStrict": true,
    "author": "Hyperledger",
    "license": "Apache-2.0",
    "dependencies": {
        "@cloudant/cloudant": "^4.2.3",
        "async": "^3.1.0",
        "axios": "^0.19.0",
        "body-parser": "^1.18.3",
        "config": "^2.0.1",
        "cookie-parser": "^1.4.3",
        "cors": "^2.8.4",
        "express": "~4.16.0",
        "fabric-ca-client": "^1.4.0",
        "fabric-client": "^1.4.8",
        "fabric-network": "1.4.0",
        "github-oauth": "^0.2.2",
        "ibm-cloud-env": "^0.2.6",
        "js-yaml": "^3.12.0",
        "jsonwebtoken": "^8.5.1",
        "lodash": "^4.17.11",
        "log4js": "^1.1.1",
        "pg": "^7.9.0",
        "request-promise": "^4.2.2",
        "swagger-ui-express": "^3.0.10"
    },
    "devDependencies": {
        "chai": "^4.2.0",
        "eslint": "^5.9.0",
        "mocha": "^5.2.0",
        "nyc": "^13.1.0",
        "sinon": "^7.1.1",
        "sinon-chai": "^3.3.0"
    },
    "nyc": {
        "exclude": [
            "coverage/**",
            "test/**"
        ],
        "reporter": [
            "text-summary",
            "html"
        ],
        "all": true,
        "check-coverage": true,
        "statements": 100,
        "branches": 100,
        "functions": 100,
        "lines": 100
    }
}
